{
  "name": "@bahutara/design-system",
  "version": "0.0.3",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/bahutara/design-system",
  "license": "MIT",
  "keywords": [
    "stitches",
    "react",
    "framer",
    "motion"
  ],
  "scripts": {
    "build": "node ./esbuild.build.js",
    "clean": "rimraf dist",
    "dev": "yarn clean && node ./esbuild.dev.js & yarn ts-types --watch",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "lint": "eslint --ext=ts,tsx .",
    "postbuild": "yarn ts-types",
    "postversion": "git push origin v$npm_package_version",
    "prebuild": "yarn clean",
    "prepack": "yarn build",
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "esbuild": "^0.14.25",
    "esbuild-plugin-glob": "^1.1.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "framer-motion": "^7.0.0",
    "prettier": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "framer-motion": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.0.0",
    "@radix-ui/react-alert-dialog": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-slider": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-toast": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@stitches/react": "^1.2.8"
  }
}
